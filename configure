  "linux/dev/include/asm" ) CONFIG_LINKS="$CONFIG_LINKS linux/dev/include/asm:linux/dev/include/asm-${systype}" ;;
  "machine" ) CONFIG_LINKS="$CONFIG_LINKS machine:${systype}/${systype}" ;;
